<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B' or '⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'Configurator component'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'⌘R'</ShortCut>
          <Description>Rerun 'Configurator component'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'configurator.compone...'">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'⌃⇧R'</ShortCut>
          <Description>Run 'configurator.compone...'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run...">
        <Item>
          <IdeaActionID>ChooseRunConfiguration</IdeaActionID>
          <ShortCut>'⌃⌥R'</ShortCut>
          <Description>Run...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>