      <option name="JSX_HTML" value="true" />
    </context>
  </template>
  <template name="rsprop" value="PropTypes.string.isRequired$END$" description="Required string PropType" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="ES6_STATEMENT" value="true" />
      <option name="JSX_HTML" value="true" />
    </context>
  </template>
  <template name="rbprop" value="PropTypes.bool.isRequired$END$" description="Required bool PropType" toReformat="true" toShortenFQNames="true">
