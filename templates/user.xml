      <option name="ES6_STATEMENT" value="true" />
      <option name="JSX_HTML" value="true" />
    </context>
  </template>
  <template name="shapeprop" value="PropTypes.shape({&#10;    $1$&#10;})$END$" description="Optional shape PropType" toReformat="true" toShortenFQNames="true">
    <variable name="1" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="ES6_STATEMENT" value="true" />
      <option name="JSX_HTML" value="true" />
    </context>
  </template>
  <template name="rshapeprop" value="PropTypes.shape({&#10;    $1$&#10;}).isRequired$END$" description="Required  shape PropType" toReformat="true" toShortenFQNames="true">
    <variable name="1" expression="" defaultValue="" alwaysStopAt="true" />
