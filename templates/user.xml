  <template name="aprop" value="PropTypes.arrayOf($1$)$END$" description="Optional array PropType" toReformat="true" toShortenFQNames="true">
    <variable name="1" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="ES6_STATEMENT" value="true" />
      <option name="JSX_HTML" value="true" />
    </context>
  </template>
  <template name="raprop" value="PropTypes.arrayOf($1$).isRequired$END$" description="Required array PropType" toReformat="true" toShortenFQNames="true">
    <variable name="1" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
