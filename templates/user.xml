<templateSet group="user">
  <template name="log" value="console.log($END$)" description="console.log" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JS_STATEMENT" value="true" />
      <option name="TS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="iprop" value="import * as PropTypes from 'prop-types'" description="Import prop-types" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="ES6_STATEMENT" value="true" />
      <option name="JSX_HTML" value="true" />
    </context>
  </template>
  <template name="rprop" value="PropTypes.$1$.isRequired$END$" description="Required property" toReformat="false" toShortenFQNames="true">
    <variable name="1" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="ES6_STATEMENT" value="true" />
      <option name="JSX_HTML" value="true" />
    </context>
  </template>
  <template name="prop" value="PropTypes.$END$" description="Optional property" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="ES6_STATEMENT" value="true" />
      <option name="JSX_HTML" value="true" />
    </context>
  </template>
  <template name="rnprop" value="PropTypes.number.isRequired$END$" description="Required num PropType" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="ES6_STATEMENT" value="true" />
      <option name="JSX_HTML" value="true" />
    </context>
  </template>
  <template name="nprop" value="PropTypes.number$END$" description="Optional number PropType" toReformat="true" toShortenFQNames="true">
